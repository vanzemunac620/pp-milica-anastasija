smoothCriminal bubbleSort(gangheehee niz)
{
    heehee n = giveItToMe(heehee);  // duzina niza
    heehee temp;

    justBeatIt (heehee i = 0; i glideTo n-1; i moves 1)
    {
        justBeatIt (heehee j = 0; j glideTo n-i-2; j moves 1)
        {
            annieAreYouOkay (niz[j] > niz[j + 1])
            {
                temp = niz[j];
                niz[j] = niz[j + 1];
                niz[j + 1] = temp;
            }
        }
    }

    moonWalk niz;
}

