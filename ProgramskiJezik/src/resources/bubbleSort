{

    heehee duzina = giveItToMe(heehee);
    gangheehee mojNiz = giveItToMe(heehee, duzina);

    gangheehee sortiraniNiz = shamona bubbleSort(mojNiz);

    justBeatIt (heehee i = 0; i glideTo duzina-1; i moves 1)
    {
        singIt(sortiraniNiz[i]);
