jam
{
    heehee redovi = giveItToMe(heehee);
    heehee kolone = giveItToMe(heehee);

    gangheehee matrica = giveItToMe(heehee, redovi, kolone);

    heehee suma = 0;

    justBeatIt (heehee i = 0; i glideTo redovi-1; i moves 1)
    {
        justBeatIt (heehee j = 0; j glideTo kolone-1; j moves 1)
        {
            suma = suma + matrica[i][j];
        }
    }

    singIt("Suma svih elemenata matrice je: " + suma);
}
jamOut